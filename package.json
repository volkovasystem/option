{
  "name": "@volkovasystem/option",
  "version": "0.0.2",
  "description": "Option class interface for procedure parameter.",
  "main": "option.js",
  "scripts": {
    "test": "node ./option.test.js",
    "clean": "rm -Rfv ./node_modules && rm ./option.js",
    "compress": "npx terser ./option.module.js --compress --comments all --output ./option.js",
    "modularize": "npx babel --plugins @babel/plugin-transform-modules-umd ./option.js --out-file ./option.js",
    "minify": "npx terser ./option.js --compress --comments all --output ./option.js",
    "build": "npm run compress && npm run modularize && npm run minify",
    "deploy": "npm run clean && npm install && npm run build && npm test",
    "prepublishOnly": "npm run clean && npm install && npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/volkovasystem/option.git"
  },
  "keywords": [
    "option",
    "parameter"
  ],
  "author": "Richeve S. Bebedor <richeve.bebedor@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/volkovasystem/option/issues"
  },
  "homepage": "https://github.com/volkovasystem/option#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-modules-umd": "^7.9.0"
  }
}
